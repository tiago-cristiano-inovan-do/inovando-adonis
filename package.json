    "copy:instructions_md": "copyfiles \"instructions.md\" build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/core": "^5.4.2",
    "@adonisjs/lucid": "^15.0.1",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.12",
    "@types/node": "^18.0.3",
    "del-cli": "^4.0.1",
    "mrm": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.1.0",
    "@adonisjs/lucid": "^15.0.1"
  },
  "adonisjs": {
    "instructionsMd": "./build/instructions.md",
    "types": "inovando-adonis",
    "providers": [
      "adonis-bumblebee-ts",
      "inovando-adonis"
    ],
    "templates": {
      "config": [
        "inovando.txt"
      ]
    }
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [],
    "minNodeVersion": "16.13.1",
    "probotApps": []
